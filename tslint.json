{
    "extends": "tslint:latest",
    "exclude": [
        "node_modules"
    ],
    "rules": {
        "one-line": false,
        "trailing-comma": "never",
        "max-classes-per-file": 3,
        "interface-name": {
            "options": [
                "never-prefix"
            ]
        }
    }
}
